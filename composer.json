{
    "name": "kaely/auth",
    "description": "Advanced authentication and authorization package for Laravel with multi-database support, OAuth integration, multitenancy, and comprehensive security features",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "authentication",
        "authorization",
        "oauth",
        "multitenancy",
        "multi-database",
        "security",
        "audit-logging",
        "session-management",
        "role-based-access-control"
    ],
    "authors": [
        {
            "name": "Kaely Team",
            "email": "dev@kaely.com",
            "homepage": "https://kaely.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.0",
        "laravel/socialite": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Kaely\\Auth\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Kaely\\Auth\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kaely\\Auth\\KaelyAuthServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/kaely/auth/issues",
        "source": "https://github.com/kaely/auth",
        "docs": "https://github.com/kaely/auth/blob/main/README.md"
    },
    "homepage": "https://github.com/kaely/auth",
    "version": "1.0.0"
} 